        <script>
        window.dataLayer = window.dataLayer || [];
        function gtag(){dataLayer.push(arguments);}
        gtag('js', new Date());

        gtag('config', 'UA-151205123-1');
        </script>
        <meta name="theme-color" content="#DEDE6C">
        <meta property="og:site_name" content="CraftOS-PC">
        <meta property="og:title" content="<%= info.title %>">
